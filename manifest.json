{
    "name": "Advent of Code Stars Order by default",
    "version": "1.0.1",
    "description": "Defaults the AoC leaderboard to 'Stars' ordering",
    "declarative_net_request" : {
        "rule_resources" : [{
          "id": "rules",
          "enabled": true,
          "path": "goToStars.json"
        }]
    },
    "permissions": [
        "declarativeNetRequest"
    ],
    "icons": {
        "16": "Stars16.png",
        "32": "Stars32.png",
        "48": "Stars48.png",
        "128": "Stars128.png"
    },
    "manifest_version": 3
}